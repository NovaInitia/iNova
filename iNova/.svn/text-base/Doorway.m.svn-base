#import "Doorway.h"

@implementation Doorway

@synthesize idNum = _idNum, user = _user, url = _url, hint = _hint, comment = _comment, nsfw = _nsfw;

- (id) initWithID:(NSUInteger)idNum user:(NSString*)user url:(NSString *)url hint:(NSString *)hint comment:(NSString *)comment nsfw:(BOOL)nsfw
{
    self = [super init];
    if (self)
    {
        _idNum = idNum;
        _user = user;
        _url = url;
        _hint = hint;
        _comment = comment;
        _nsfw = nsfw;
    }
    return self;
}

@end
